<h1 class="pagetitle">Entries</h1>
<h1 class="block">Editing Entry</h1>

<h2>
<b>Tournament:</b>
  <%=h @entry.tournament.league.name %> @ <%=h @entry.tournament.formatted_start %>
</h2>

<h2>
  <b>Player:</b>
  <%=h @entry.player.first %> <%=h @entry.player.last %>
</h2>

<h2>
  <b>Paid:</b>
  <%=h @entry.paid %>
</h2>

<h2>
  <b>Result:</b>
  <%=h @entry.result %>
</h2>

<h2>
  <b>Payout:</b>
  <%=h @entry.payout %>
</h2>

<% if logged_in? %>
	<%= link_to 'Edit', edit_entry_path(@entry) %> |
<% end %>
<%= link_to 'List of All Entries', entries_path %>
