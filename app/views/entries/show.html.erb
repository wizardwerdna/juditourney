<h1 class="pagetitle">Tournament Entry</h1>

<h2>
<b>Tournament:</b>
  <%=h @entry.tournament.league.name %> @ <%=h @entry.tournament.start %>
</h2>

<h2>
  <b>Player:</b>
  <%=h @entry.player.first %> <%=h @entry.player.last %>
</h2>

<h2>
  <b>Paid:</b>
  <%=h @entry.paid %>
</h2>

<h2>
  <b>Result:</b>
  <%=h @entry.result %>
</h2>

<h2>
  <b>Payout:</b>
  <%=h @entry.payout %>
</h2>


<%= link_to 'Edit', edit_entry_path(@entry) %> |
